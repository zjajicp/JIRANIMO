{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "globals": {
    "Jira": true,
    "Ajax": true,
    "Rx": true,
    "getBasicAuthentication": true,
    "btoa": true,
    "JIRA_STATUSES": true,
    "chrome": true,
    "fetch": true,
    "XMLHttpRequest": true,
    "Stash": true,
    "document": true,
    "Notifier": true,
