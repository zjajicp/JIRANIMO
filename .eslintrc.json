{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "globals": {
    "Jira": true,
    "Ajax": true,
    "Rx": true,
    "Utils": true,
    "btoa": true,
    "JiraStatuses": true,
    "chrome": true,
    "fetch": true,
    "XMLHttpRequest": true,
    "Stash": true,
    "document": true,
    "Notifier": true,
    "ConfigLoader": true,
    "Jenkins": true,
    "branchToJobMapper": true,
    "UnmergedList": true,
    "TicketUpdater": true,
    "PoolRequestHandler": true,
    "MergeHandler": true,
    "ConfigMapper": true,
    "ISSUE_TYPES": true,
    "WorkflowPaths": true
  },
   "rules": {
       "comma-dangle": "off",
       "prefer-arrow-callback": "off",
       "func-names": "off"
   }
}